{
    "material-icon-theme.activeIconPack": "nest",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always",
    },
    "editor.formatOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "workbench.colorCustomizations": {
        "tab.activeBackground": "#01455e",
        "tab.activeForeground": "#d1b200",
    
        "activityBar.activeBackground": "#580016",
        "activityBar.background": "#00435c",
        "activityBar.foreground": "#e7e7e7",
        "activityBar.inactiveForeground": "#e7e7e799",
        "activityBarBadge.background": "#422c74",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#e7e7e799",
        "sash.hoverBorder": "#2f7c47",
        "statusBar.background": "#ac3701",
        "statusBar.foreground": "#e7e7e7",
        "statusBarItem.hoverBackground": "#2f7c47",
        "statusBarItem.remoteBackground": "#215732",
        "statusBarItem.remoteForeground": "#e7e7e7",
        "titleBar.activeBackground": "#220101",
        "titleBar.activeForeground": "#e7e7e7",
        "titleBar.inactiveBackground": "#21573299",
        "titleBar.inactiveForeground": "#e7e7e799"
    },
    "peacock.remoteColor": "#215732",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    }
}